# Gemini API (Google AI Studio)
GOOGLE_API_KEY=your_google_ai_studio_api_key_here
GEMINI_MODEL=gemini-2.5-flash

# Backend mock server
BACKEND_URL=http://localhost:8081

# Redis session store
REDIS_URL=redis://localhost:6379/0
SESSION_TTL_MIN=60

# Rate limiting
RATE_LIMIT_PER_MIN=30

# Logging
APP_ENV=development  # development / production (affects log format and level)
LOG_LEVEL=DEBUG      # DEBUG / INFO / WARNING / ERROR (optional, auto-detected from APP_ENV if not set)
